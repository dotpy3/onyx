{% extends 'SDFBilletterieBundle:Pages/Admin/Entities:new.html.twig' %}

{% block entity_title %}Billet{% endblock %}

{% block entity_create_path %}{{ path('sdf_billetterie_administration_ticket_create') }}{% endblock %}

{% block form_content %}
	<div class='row'>
		<div class='col-sm-6'>{{ form_row(form.prenom) }}</div>
		<div class='col-sm-6'>{{ form_row(form.nom) }}</div>
	</div>
	<div class='row'>
		<div class='col-sm-6'>{{ form_row(form.idPayutc) }}</div>
		<div class='col-sm-6'>{{ form_row(form.barcode) }}</div>
	</div>
	<div class='row'>
		<div class='col-sm-6'>{{ form_row(form.navette) }}</div>
		<div class='col-sm-6'>
			{{ form_row(form.accepteDroitImage) }}
			{{ form_row(form.isMajeur) }}
			{{ form_row(form.valide) }}
		</div>
	</div>
	<div class='row'>
		<div class='col-sm-6'>{{ form_row(form.user) }}</div>
		<div class='col-sm-6'>{{ form_row(form.tarif) }}</div>
	</div>
	{{ form_rest(form) }}
{% endblock %}

{% block entity_list_path %}{{ path('sdf_billetterie_administration_tickets_list') }}{% endblock %}